package practice;
import java.util.Scanner;
public class practice
{
public static void main(String[]args ) {
int arr[] = {7, 10, 4, 10, 6, 5, 2};
int i;
int t= arr[arr.length-1];

System.out.println(t);

for(i=arr.length-2 ; i>=0 ;i--) {
if(t<arr[i]) {
t= arr[i];

System.out.println(t);
     } 
     } 
     } 
     }
