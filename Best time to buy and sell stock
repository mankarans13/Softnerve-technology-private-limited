package practice;
import java.util.Scanner;
public class practice
{
	public static void main(String[]args ) {
		int arr[] = {7,6,4,3,1};
	int	 i,j=1;
	int min= arr[0]; 

		for(i=1 ; i<arr.length ;i++) {
				if(min>arr[i]) {
					min = arr[i];
					j=i;
				}
		}	
		int max= min;
		for(int k=j;k<arr.length;k++) {
			if(max<arr[k]) {
				max= arr[k];
			}
		}
		int profit=0;
		profit= max-min;
		System.out.println(profit);
		} 
	}
